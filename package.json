  "license": "MIT",
  "bugs": {
    "url": "https://github.com/keenondrums/flux-action-class/issues"
  },
  "homepage": "https://github.com/keenondrums/flux-action-class#readme",
  "devDependencies": {
    "@types/jest": "^24.0.6",
    "@types/shortid": "0.0.29",
    "coveralls": "^3.0.3",
    "doctoc": "^1.4.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "shx": "^0.3.2",
    "ts-jest": "^24.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
